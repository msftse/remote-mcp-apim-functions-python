# Azure AI Foundry project endpoint
AZURE_AI_PROJECT_ENDPOINT=https://<your-resource>.services.ai.azure.com/api/projects/<your-project>

# Model deployment name (must be deployed in your AI Foundry project)
MODEL_DEPLOYMENT_NAME=gpt-4o

# APIM gateway base URL (from azd output or Azure Portal)
APIM_GATEWAY_URL=https://<your-apim-name>.azure-api.net

# GitHub MCP server (remote hosted, no APIM proxy needed)
GITHUB_MCP_URL=https://api.githubcopilot.com/mcp/

# Name of the Custom Keys connection in AI Foundry for GitHub PAT auth
GITHUB_MCP_CONNECTION_NAME=github-mcp
